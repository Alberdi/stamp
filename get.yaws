  "<erl>
    out(A) -> [{"tag", Tag}] = yaws_api:parse_query(A), {ehtml, stamp_server:normalize(tag, Tag)}.
  </erl>"
    name="tag" id="tag" />
  <input type="text" name="msg" id="msg"><br>
