<ol>
  <erl>
    out(A) ->
      [{"tag", Tag}] = yaws_api:parse_query(A),
      {ehtml, [{li, [], X} || X <- stamp_server:get(Tag, 50)]}.
  </erl>
</ol>

<h2>Stamp on this tag</h2>
